<h1>会員一覧</h1>
<table class="table">
	<thead>
		<tr>
			<th>バンド名</th>
			<th>作成日時</th>
	    </tr>
    </thead>
    <tbody>
       <% @users.each do |user| %>
		 	<tr>
		 		<td>
		 			<%= link_to user.last_name + user.first_name, admin_user_path(user) %>
		 		</td>
		 		<td>
		 			<%= user.created_at %>
		 		</td>
		 	</tr>
	  <% end %>
    </tbody>
 </table>
